bind J jsb browser.tabs.query({currentWindow: true, hidden: false}).then(tabs => (tabs.sort((a, b) => a.index - b.index), tabs)).then(tabs => browser.tabs.update(tabs[(tabs.findIndex(t => t.active) + 1) % tabs.length].id, {active: true}))

bind K jsb browser.tabs.query({currentWindow: true, hidden: false}).then(tabs => (tabs.sort((a, b) => a.index - b.index), tabs)).then(tabs => browser.tabs.update(tabs[(tabs.findIndex(t => t.active) - 1 + tabs.length) % tabs.length].id, {active: true}))
